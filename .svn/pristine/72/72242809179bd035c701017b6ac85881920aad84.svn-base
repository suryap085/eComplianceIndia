package org.opasha.eCompliance.ecompliance.Model;

public class Reason {
	public int Id;
	public String reason;
	public int IsActive;

	public Reason(int Id, String reason) {
		this.Id = Id;
		this.reason = reason;
	}

	public void setReason(String reason) {
		this.reason = reason;
	}

	public void setReason(int Id, String reason, int IsActive) {
		this.Id = Id;
		this.reason = reason;
		this.IsActive = IsActive;

	}

	public String toString() {
		return reason;
	}

	public void setReason(int Id, String reason) {
		this.Id = Id;
		this.reason = reason;

	}

}
